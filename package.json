{
  "name": "netease-music-cloud-uploader",
  "private": true,
  "version": "1.2.2",
  "type": "module",
  "scripts": {
    "start": "vite",
    "dev": "tauri dev",
    "build:web": "tsc && vite build",
    "build": "tauri build --target x86_64-apple-darwin",
    "tauri": "tauri",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,scss}\"",
    "lint": "eslint --config .eslintrc.commit.json \"./src/**/*.{ts,tsx}\"",
    "prepare": "husky install"
  },
  "dependencies": {
    "@floating-ui/react": "^0.26.5",
    "@tanstack/react-query": "^5.17.7",
    "@tanstack/react-query-devtools": "^5.17.7",
    "@tauri-apps/api": "^1.5.3",
    "buffer": "^6.0.3",
    "bytes": "^3.1.2",
    "clsx": "^2.1.0",
    "date-fns": "^3.1.0",
    "immer": "^10.0.3",
    "lodash.get": "^4.4.2",
    "lodash.uniqby": "^4.7.0",
    "lodash.uniqwith": "^4.5.0",
    "music-metadata": "^8.1.4",
    "music-metadata-browser": "^2.5.10",
    "qrcode": "^1.5.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "react-router-dom": "^6.21.1",
    "spark-md5": "^3.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@tanstack/eslint-plugin-query": "^5.17.7",
    "@tauri-apps/cli": "^1.5.9",
    "@types/bytes": "^3.1.4",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.uniqby": "^4.7.9",
    "@types/lodash.uniqwith": "^4.5.9",
    "@types/node": "^20.10.6",
    "@types/qrcode": "^1.5.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@types/spark-md5": "^3.0.4",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.1",
    "sass": "^1.69.7",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vite-plugin-checker": "^0.6.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --config .eslintrc.commit.json"
    ],
    "*.{css,scss}": [
      "prettier --write"
    ]
  }
}
